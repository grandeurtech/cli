{
  "name": "grandeur-cli",
  "description": "This command line interface has been designed to let users work with Grandeur over command prompt",
  "version": "1.0.1",
  "author": "Grandeur Technologies <hi@grandeur.tech>",
  "bin": {
    "grandeur": "./bin/run"
  },
  "bugs": "https://github.com/grandeurtech/cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "chalk": "^4.0.0",
    "cli-ux": "^5.4.5",
    "cors": "^2.8.5",
    "createhmac-chaintor": "^1.0.0",
    "express": "^4.17.1",
    "figlet": "^1.4.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://grandeur.tech",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "grandeur",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "grandeurtech/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
