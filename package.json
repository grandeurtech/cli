{
  "name": "gApollo",
  "description": "This command line interface has been designed to let users work with Grandeur Cloud over command prompt",
  "version": "0.0.0",
  "author": "Grandeur Technologies <hi@grandeur.tech>",
  "bin": {
    "gApollo": "./bin/run"
  },
  "bugs": "https://github.com/grandeurtech/grandeurcloud-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://grandeur.tech",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "gApollo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/grandeurtech/grandeurcloud-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
